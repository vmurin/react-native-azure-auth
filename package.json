    "babel-jest": "^24.8.0",
    "codecov": "^3.5.0",
    "eslint": "^6.0.1",
    "eslint-plugin-jest": "^22.7.1",
    "eslint-plugin-react": "^7.14.2",
    "faker": "^4.1.0",
    "fetch-mock": "^7.3.3",
    "jest": "^24.8.0",
    "jetifier": "^1.6.6",
    "jsdoc": "^3.6.2",
    "jsdoc-to-markdown": "^5.0.0",
